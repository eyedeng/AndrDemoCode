<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andrdemocode">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndrDemoCode">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".timer.CountActivity" />
        <activity android:name=".grouprv.GroupRVActivity" />
        <activity android:name=".rahul.MainActivity" />
        <activity
            android:name=".tabview.MultiMusicSourceCollectionActivity"
            android:exported="true" />
        <activity
            android:name=".lauchmode.AActivity"
            android:exported="false" />
        <activity
            android:name=".lauchmode.RouterActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="adc.com"
                    android:pathPrefix="/path"
                    android:scheme="adc" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ssl.SingleSelectListActivity"
            android:exported="false" />
        <activity
            android:name=".architecture.mvvm.PatternsActivity"
            android:exported="false" />
    </application>

</manifest>